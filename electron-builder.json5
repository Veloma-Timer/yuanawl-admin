{
  "$schema": "https://raw.githubusercontent.com/electron-userland/electron-builder/master/packages/app-builder-lib/scheme.json",
  "appId": "com.yuanawl.c.1.0.0",
  "asar": false,
  "productName": "元阿网络",
  "directories": {
    "output": "release/${version}"
  },
  "files": [
    "./dist",
    "./dist-electron"
  ],
  "publish": {
    "provider": "github",
    "owner": "Veloma-Timer",
    "repo": "yuanawl-admin",
    "token": "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",
    "releaseType": "release"
  },
  "mac": {
    "icon": "./public/favicon.png",
    "target": [
      "dmg"
    ],
    "artifactName": "${productName}-Mac-${version}-Installer.${ext}"
  },
  "win": {
    "icon": "./public/favicon_256.ico",
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      }
    ],
    "artifactName": "${productName}-Windows-${version}-Setup.${ext}"
  },
  "nsis": {
    "oneClick": false,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "installerIcon": "./public/favicon_256.ico",
    "uninstallerIcon": "./public/favicon_256.ico",
    "installerHeaderIcon": "./public/favicon_256.ico",
    "createDesktopShortcut": true,
    "createStartMenuShortcut": true,
    "shortcutName": "元阿网络"
  }
}
